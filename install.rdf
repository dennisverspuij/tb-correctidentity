<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">

    <em:id>{47ef7cc0-2201-11da-8cd6-0800200c9a66}</em:id>
    <em:name>Correct Identity</em:name>
    <em:version>1.4.8</em:version>
    <em:type>2</em:type>
    <em:description>Allows you to configure the identity that will be chosen upon composing mail.</em:description>
    <em:creator>Dennis Verspuij</em:creator>
    <em:contributor>Benjamin Menant (fr-FR locale)</em:contributor>
    <em:contributor>Alan Boey (TB3 compatibility)</em:contributor>
    <em:contributor>Tobias Markus (de-DE locale)</em:contributor>
    <em:contributor>Premier.jp (ja locale)</em:contributor>
    <em:contributor>Robert Orzanna (Postbox compatibility)</em:contributor>
    <em:contributor>azurewelkin (TB v13+ compatibility fix)</em:contributor>
    <em:contributor>Andreas Wagner (TB v20+ compatibility)</em:contributor>
    <em:contributor>Axel Grude (TB v20+ compatibility)</em:contributor>
    <em:contributor>Petr Vones (TB v20+ compatibility)</em:contributor>
    <em:contributor>Mitar Milutinovic (as-you-type indentity and aliases matching for new messages)</em:contributor>
    <em:contributor>Bojan Čekrlić (warn when sending to certain recipients)</em:contributor>
    <em:contributor>Ludger (TB v52+ compatibility))</em:contributor>
    <em:contributor>Martin Zillmer (TB v57+ compatibility))</em:contributor>
    <em:optionsURL>chrome://correctidentity/content/options.xul</em:optionsURL>
    <em:homepageURL/>

    <!-- Thunderbird -->
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>1.0</em:minVersion>
        <em:maxVersion>60.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Postbox -->
     <em:targetApplication>
      <Description>
        <em:id>postbox@postbox-inc.com</em:id>
        <em:minVersion>2.0b1</em:minVersion>
        <em:maxVersion>3.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:file>
      <Description about="urn:mozilla:extension:file:correctidentity.jar">
        <em:package>content/</em:package>
        <em:skin>skin/classic/</em:skin>
        <em:locale>locale/en-US/</em:locale>
        <em:locale>locale/nl-NL/</em:locale>
        <em:locale>locale/de-DE/</em:locale>
        <em:locale>locale/ja/</em:locale>
        <em:locale>locale/fr-FR/</em:locale>
      </Description>
    </em:file>

  </Description>
</RDF>
